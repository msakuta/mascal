struct Complex {
    re: f64,
    im: f64,
}

fn format_complex(s: Complex) -> str {
    (s.re) as str + ", " + (s.im) as str
}

fn mult(a: Complex, b: Complex) -> Complex {
    Complex {
        re: a.re * b.re - a.im * b.im,
        im: a.re * b.im + a.im * b.re,
    }
}

var a = Complex { re: 1., im: 1.};
var b = Complex { re: -1., im: 1.};
var c = mult(a, b);

print(format_complex(c))
