fn g(x: f64) -> f64 {
    x * 1.5;
}

print(g(x: 10.3));

fn h(x: i64) -> i64 {
    x * 10;
}

print(h(x : 103));

fn digits(a: f64, b: f64) -> f64 {
    a + b * 10.
}

print(digits(a: 1. + 2., b: 5.));
print(digits(b: 1. + 2., a: 5.));
